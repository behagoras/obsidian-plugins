{
  "name": "typora_plugin",
  "version": "1.0.0",
  "author": "David Behar Lombrozo",
  "description": "An Obsidian plugin that inserts two spaces on Enter and one space on Shift+Enter.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && webpack",
    "watch": "tsc --watch",
    "deploy": "rm -rf ~/Library/Mobile\\ Documents/iCloud\\~md\\~obsidian/Documents/Personal/.obsidian/plugins/typora-like && cp -r dist ~/Library/Mobile\\ Documents/iCloud\\~md\\~obsidian/Documents/Personal/.obsidian/plugins/typora-like",
    "build:deploy": "npm run build && npm run deploy"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "obsidian": "latest",
    "typescript": "^4.5.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
